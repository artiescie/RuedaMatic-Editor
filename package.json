{
  "name": "ruedamatic-editor",
  "version": "8.0.0",
  "author": "rc <rcurzon12@gmail.com>",
  "description": "manages the data files for rueda caller ruedamatic editor, and rm-spot - rueda calls for spotify",
  "license": "tba",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "./fixdate.sh && node .electron-vue/build.js && electron-builder",
    "build:all": "node .electron-vue/build.js && electron-builder -mwl",
    "build:windows": "./fixdate.sh && node .electron-vue/build.js && electron-builder -w",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "ruedamatic-editor",
    "appId": "com.come2think.ruedamatic-editor",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@andi23rosca/vue-pan-zoom": "^1.0.3",
    "adm-zip": "^0.4.13",
    "archiver": "^3.1.1",
    "axios": "^0.16.1",
    "basic-ftp": "^3.5.0",
    "bootstrap-vue": "^2.1.0",
    "chalk": "^1.1.3",
    "deep-diff": "^0.3.8",
    "diacritic-regex": "^1.1.0",
    "diff": "^4.0.1",
    "electron-store": "^2.0.0",
    "ffbinaries": "^1.1.1",
    "folder-hash": "^3.1.0",
    "fs-extra": "^3.0.1",
    "glob": "^8.0.3",
    "he": "^1.2.0",
    "howler": "^2.2.3",
    "jquery": "^3.3.1",
    "js-yaml": "^3.13.1",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.10",
    "mermaid": "^8.14.0",
    "minimist": "^1.2.7",
    "mkdirp": "^1.0.4",
    "opn": "^5.3.0",
    "replace-in-file": "^4.3.1",
    "smooth-scroll-into-view-if-needed": "^1.1.23",
    "trash": "^8.1.0",
    "upath": "^2.0.1",
    "vee-validate": "^2.1.0-beta.5",
    "vue": "^2.5.16",
    "vue-awesome": "^4.0.2",
    "vue-electron": "^1.0.6",
    "vue-file-picker": "0.0.2",
    "vue-i18n": "^8.9.0",
    "vue-router": "^2.8.1",
    "vue-spinner": "^1.0.3",
    "vue2-simplert-plugin": "^0.4.1",
    "vuex": "^2.5.0",
    "wavesurfer.js": "^2.2.1",
    "wget-improved": "^3.0.2",
    "winston": "^3.8.2",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@types/lodash": "^4.14.110",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
